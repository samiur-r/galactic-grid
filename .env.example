# Space API Keys (optional but recommended for higher rate limits)
NASA_API_KEY=your_nasa_api_key_here
LAUNCH_LIBRARY_API_KEY=your_launch_library_key_here
N2YO_API_KEY=your_n2yo_satellite_api_key_here

# OpenAI API Key for AI SDK
OPENAI_API_KEY=your_openai_api_key_here

# MCP Server Configuration
MCP_SERVER_PORT=3001
MCP_SERVER_HOST=localhost

# MCP Client Configuration
MCP_CLIENT_TRANSPORT=http
MCP_CLIENT_TIMEOUT=30000

# Redis Configuration (required for SSE transport)
